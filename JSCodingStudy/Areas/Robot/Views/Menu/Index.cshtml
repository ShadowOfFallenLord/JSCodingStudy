@model IEnumerable<LessonInfo>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Robot Menu";
}


@Html.ActionLink("- Sandbox", "Sandbox", "Lesson")
<br />
@foreach (LessonInfo info in Model)
{
    string title = $"- Lesson {info.Id} - {info.Title}";
    @Html.ActionLink(title, "Lesson", "Lesson", new { id = info.Id }, null)
    <br />
}

<br />
@Html.ActionLink("- Back", "Index", "Home", new { area = "" }, null)
